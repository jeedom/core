{
  "require": {
    "dragonmantank/cron-expression": "^3",
    "symfony/expression-language": "5 - 7",
    "pragmarx/google2fa-qrcode": "^3",
    "bacon/bacon-qr-code": "2 - 3",
    "influxdata/influxdb-client-php": "^3",
    "psr/log": "^1.1",
    "php": ">=7.4"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    },
    "platform": {
      "php": "7.4"
    }
  },
  "autoload": {
    "classmap": [
      "core/class/",
      "core/com/",
      "core/repo/"
    ],
    "files": [
      "core/config/common.config.php",
      "core/config/compatibility.config.php",
      "core/config/jeedom.config.php",
      "core/php/utils.inc.php"
    ]
  }
}
